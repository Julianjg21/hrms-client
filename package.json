{
  "name": "hrms-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.7.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@popperjs/core": "^2.11.8",
    "@sentry/cli": "^2.42.3",
    "@sentry/react": "^9.5.0",
    "axios": "^1.7.8",
    "bootstrap": "^5.3.3",
    "crypto-js": "^4.2.0",
    "normalize.css": "^8.0.1",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.5",
    "react-currency-input-field": "^3.9.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^7.0.1"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.28.0",
    "gh-pages": "^4.0.0",
    "react-scripts": "^5.0.1",
    "webpack": "^5.96.1"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build && npm run sentry:sourcemaps",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org julian-vv --project hrms-client ./build && sentry-cli sourcemaps upload --org julian-vv --project hrms-client ./build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}